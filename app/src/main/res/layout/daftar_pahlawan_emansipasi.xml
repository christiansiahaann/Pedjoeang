<?xml version="1.0" encoding="utf-8"?>
<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color3"
    tools:context="com.prakpapb2.intro1.DaftarPahlawanEmansipasi">

    <ImageButton
        android:id="@+id/ib_back"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_gravity="center"
        android:layout_marginStart="28dp"
        android:layout_marginTop="60dp"
        android:background="@android:color/transparent"
        android:src="@drawable/ic_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <com.veeyaar.supergradienttextview.GradientTextView
        android:id="@+id/textJenisPahlawan"
        android:layout_width="235dp"
        android:layout_height="31dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="30dp"
        android:width="16dp"
        android:height="26dp"
        android:autoSizeMaxTextSize="22sp"
        android:fontFamily="@font/poppins"
        android:text="@string/emansipasi_wanita"
        android:textAllCaps="false"
        android:textSize="22dp"
        android:textStyle="bold"
        app:endColor="@color/color5"
        app:isVertical="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.565"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:startColor="@color/color4"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/gambarpahlawan1"
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="72dp"
        android:src="@drawable/gambar_ra_kartini"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textJenisPahlawan"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto"
        android:text="R.A. Kartini"
        android:textColor="#000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gambarpahlawan1"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/gambarpahlawan2"
        android:layout_width="149dp"
        android:layout_height="146dp"
        android:layout_marginTop="72dp"
        android:src="@drawable/gambar_cut_nyak_dhien"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toEndOf="@+id/gambarpahlawan1"
        app:layout_constraintTop_toBottomOf="@+id/textJenisPahlawan"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto"
        android:text="Cut Nyak Dhien"
        android:textColor="#000"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/gambarpahlawan2"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/gambarpahlawan3"
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/gambar_fatmawati"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView1"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto"
        android:text="Fatmawati"
        android:textColor="#000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gambarpahlawan3"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/gambarpahlawan4"
        android:layout_width="149dp"
        android:layout_height="146dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/gambar_dewi_sartika"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toEndOf="@+id/gambarpahlawan3"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="83dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto"
        android:text="Raden Dewi Sartika"
        android:textColor="#000"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/gambarpahlawan4"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/gambarpahlawan5"
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/gambar_cut_meutia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto"
        android:text="Cut Meutia"
        android:textColor="#000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gambarpahlawan5"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>